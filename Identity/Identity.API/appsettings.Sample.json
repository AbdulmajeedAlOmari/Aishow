{
    "MessageBroker": {
        "ConnectionString": "PUT_RABBITMQ_CONNECTION_STRING"
    },
    "ConnectionStrings": {
        "DefaultConnection": "PUT_DB_CONNECTION_STRING_HERE"
    },
    "AppSettings": {
        "Secret": "PUT_SECRET_HERE"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Sentry": {
        "Dsn": "PUT_URL_HERE",
        "MaxRequestBodySize": "Always",
        "SendDefaultPii": true,
        "MinimumBreadcrumbLevel": "Debug",
        "MinimumEventLevel": "Warning",
        "AttachStackTrace": true,
        "Debug": true,
        "DiagnosticsLevel": "Error"
    }
}
  